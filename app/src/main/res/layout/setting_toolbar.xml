<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="?actionBarSize"
    android:padding="@dimen/_10mdp"
    android:elevation="@dimen/_5mdp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/setting_toolbar_back"
        android:layout_width="@dimen/_24mdp"
        android:layout_height="@dimen/_24mdp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/fragment_setting_toolbar_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/textView"
        android:textSize="@dimen/_12mdp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="@dimen/_20mdp"
        android:layout_marginRight="@dimen/_20mdp"
        android:layout_marginStart="@dimen/_20mdp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/setting_toolbar_back"
        app:layout_constraintStart_toEndOf="@id/setting_toolbar_back">
    </TextView>


</androidx.constraintlayout.widget.ConstraintLayout>